<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="roshanmanjaly.cs301.cs.wm.edu.amazebyroshanmanjaly.ui">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".gui.AMazeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".gui.GeneratingActivity" />
        <activity android:name=".gui.FinishActivity" />
        <activity android:name=".gui.PlayAutoActivity" />
        <activity android:name=".gui.PlayManualActivity"></activity>
    </application>

</manifest>